-extends "layouts/dashboard.haml"

-block title
  Mockups Sample

-block header
  -include 'base_objects/_page_header.haml'

-block nav
  .top-inner-nav
    .object-nav
      %ul.nav.internav

-block main
  .clearfix
    .object-area
      #show-columns.column-set
        #middle-column
          .row-fluid
            .span12.full-width
              .inner-content

-block extra_templates
  -assets "mockup-js-templates"
    %script{ type:'text/javascript', src:"={ASSET_URL|with_static_subdomain}" }
  -endassets

-block extra_assets
  -assets "mockup-js"
    %script{ type:'text/javascript', src:"={ASSET_URL|with_static_subdomain}" }
  -endassets
